<view class="container">
	<!--定义页面结构，使用文本组件或其他组件 -->
	<view class='title-view'>
		<text>规章制度</text>
	</view>

	<view class="divLine"></view>

	<view class="all-text-view">
		<view class="text-title">
			<text>上海市轨道交通乘客守则</text>
		</view>
		<view class="text-view">
			<block wx:for='{{regulations}}' wx:for-index="index">
